{
    "name": "lightproxy",
    "version": "1.0.0",
    "main": "dist/main/main.js",
    "repository": "git@github.com:alibaba/lightproxy.git",
    "license": "Apache License 2.0",
    "scripts": {
        "dev:main": "tsc -w -p tsconfig.main.json",
        "dev:renderer": "tsc -w -p tsconfig.renderer.json",
        "dev": "npx npm-run-all copy:web_modules syncversion --parallel dev:main dev:renderer",
        "build:main": "tsc -p tsconfig.main.json",
        "build:renderer": "tsc -p tsconfig.renderer.json",
        "build": "npx npm-run-all clean build:main build:render",
        "dist": "yarn run build && yarn run syncversion",
        "clean": "npx npm-run-all clean:dist clean:web_modules",
        "clean:dist": "npx rimraf app/dist",
        "clean:web_modules": "npx rimraf app/web_modules",
        "copy:web_modules": "cp -r web_modules app/web_modules",
        "syncversion": "node scripts/sync-version.js",
        "postinstall": "npx snowpack"
    },
    "devDependencies": {
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "electron": "^8.0.0-beta.5",
        "electron-builder": "^22.2.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.17.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2",
        "snowpack": "^1.7.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "react": "npm:@pika/react",
        "react-dom": "npm:@pika/react-dom"
    }
}
